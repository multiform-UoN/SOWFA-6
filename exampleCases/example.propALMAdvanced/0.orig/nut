FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -1 0 0 0 0 ];

internalField   uniform 1e-8;

boundaryField
{
    INLET
    {
        type            fixedValue;
	value		$internalField;
    }
    OUTLET
    {
        type            zeroGradient;
    }

    WALLS
    {
	type		zeroGradient;
    }

    "proc.*"
    {
    	type            processor;
    }
}


